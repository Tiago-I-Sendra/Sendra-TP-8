    cout<<"ingresar jornada laboral";
    cin>>Horario;

    cout<<"su salario es";

    if(Horario == 8 || Horario == 9){cout<<"250000"endl;}
    else if(Horario == 10 || Horario == 11){cout<<"300000"endl;}
    else if(Horario => 12){cout<<"350000"<<endl;}
    };

	};

int main(int argc, char *argv[])
{
	Persona Zona;
